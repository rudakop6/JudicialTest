<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:JudicialTest">

    <Thickness x:Key="ComboBox_PropertiesEntity.Margin" Bottom="5" Left="5" Right="5" Top="5"/>
    <Style x:Key="ComboBox_PropertiesEntity.Style" TargetType="ComboBox">
        <Setter Property="Margin" Value="{StaticResource ComboBox_PropertiesEntity.Margin}"/>
        <Setter Property="FontSize" Value="20"/>
    </Style>
</ResourceDictionary>