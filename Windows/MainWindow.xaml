<Window x:Class="JudicialTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JudicialTest"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        mc:Ignorable="d"
        Title="Главное окно" Height="775" Width="1600" FontSize="16"  WindowState="Maximized" MinHeight="600" MinWidth="1280">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resourses/ButtonStyles.xaml"/>
                <ResourceDictionary Source="Resourses/TextBlockStyles.xaml"/>
                <ResourceDictionary Source="Resourses/DataGridStyles.xaml"/>
                <ResourceDictionary Source="Resourses/TextBoxStyles.xaml"/>
                <ResourceDictionary Source="Resourses/ScrollViewerStyles.xaml"/>
                <ResourceDictionary Source="Resourses/ExpanderStyles.xaml"/>
                <ResourceDictionary Source="Resourses/ComboBoxStyles.xaml"/>
                <ResourceDictionary Source="Resourses/BorderStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <local:PeriodMonthConverter x:Key="monthConverter"/>
        </ResourceDictionary>
    </Window.Resources>
    <Grid Name="MainMenuForm" Visibility="Visible" Background="DarkSlateGray">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="8*"/>
            <!--<ColumnDefinition Width="1*"/>-->
        </Grid.ColumnDefinitions>

        <!--<Grid Grid.Column="2">
            <TextBox Grid.Row="0" Name="DEBUG" Style="{StaticResource TextBox_DEBUG.Style}"/>
        </Grid>-->

        <Grid Grid.Column="0">
            <Grid.RowDefinitions>
                <RowDefinition Height="5*"/>
                <RowDefinition Height="1*"/>

                <RowDefinition Height="4*"/>
                <RowDefinition Height="1*"/>
                <RowDefinition Height="1*"/>
                <RowDefinition Height="1*"/>
            </Grid.RowDefinitions>

            <Grid Grid.Row="0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="1*"/>
                </Grid.RowDefinitions>
                <Button Grid.Row="0" Name="Button_Houses" Content="Дома"  Command="{Binding HouseCommand}">
                    <Button.Style>
                        <Style TargetType="Button" BasedOn="{StaticResource Button_Menu.Style}">
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding ElementName=HousesForm, Path=Visibility}" Value="Hidden">
                                    <Setter Property="Background" Value="{StaticResource Button_Menu.Color1}"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=HousesForm, Path=Visibility}" Value="Visible">
                                    <Setter Property="Background" Value="{StaticResource Button_Menu.Color2}"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Style>
                </Button>

                <Button Grid.Row="1" Name="Button_Apartments" Content="Помещения" Command="{Binding ApartmentCommand}">
                    <Button.Style>
                        <Style TargetType="Button" BasedOn="{StaticResource Button_Menu.Style}">
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding ElementName=ApartmentsForm, Path=Visibility}" Value="Hidden">
                                    <Setter Property="Background" Value="{StaticResource Button_Menu.Color1}"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=ApartmentsForm, Path=Visibility}" Value="Visible">
                                    <Setter Property="Background" Value="{StaticResource Button_Menu.Color2}"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Style>
                </Button>
                <Button Grid.Row="2" Name="Button_Debtors" Content="Должники" Command="{Binding DebtorCommand}">
                    <Button.Style>
                        <Style TargetType="Button" BasedOn="{StaticResource Button_Menu.Style}">
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding ElementName=DebtorsForm, Path=Visibility}" Value="Hidden">
                                    <Setter Property="Background" Value="{StaticResource Button_Menu.Color1}"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=DebtorsForm, Path=Visibility}" Value="Visible">
                                    <Setter Property="Background" Value="{StaticResource Button_Menu.Color2}"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Style>
                </Button>
                <Button Grid.Row="3" Name="Button_Extracts" Content="Выписки ЕГРН" Command="{Binding ExtractCommand}">
                    <Button.Style>
                        <Style TargetType="Button" BasedOn="{StaticResource Button_Menu.Style}">
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding ElementName=ExtractsForm, Path=Visibility}" Value="Hidden">
                                    <Setter Property="Background" Value="{StaticResource Button_Menu.Color1}"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=ExtractsForm, Path=Visibility}" Value="Visible">
                                    <Setter Property="Background" Value="{StaticResource Button_Menu.Color2}"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Style>
                </Button>
                <Button Grid.Row="4" Name="Button_Arrears" IsEnabled="False" Content="Задолженности" Command="{Binding ArrearCommand}">
                    <Button.Style>
                        <Style TargetType="Button" BasedOn="{StaticResource Button_Menu.Style}">
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding ElementName=ArrearsForm, Path=Visibility}" Value="Hidden">
                                    <Setter Property="Background" Value="{StaticResource Button_Menu.Color1}"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=ArrearsForm, Path=Visibility}" Value="Visible">
                                    <Setter Property="Background" Value="{StaticResource Button_Menu.Color2}"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Style>
                </Button>
            </Grid>
            <Grid Grid.Row="5">
                <Button Content="Настройки" Command="{Binding SettingsCommand}">
                    <Button.Style>
                        <Style TargetType="Button" BasedOn="{StaticResource Button_Menu.Style}">
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding ElementName=SettingsForm, Path=Visibility}" Value="Hidden">
                                    <Setter Property="Background" Value="{StaticResource Button_Menu.Color1}"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=SettingsForm, Path=Visibility}" Value="Visible">
                                    <Setter Property="Background" Value="{StaticResource Button_Menu.Color2}"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Style>
                </Button>
            </Grid>
            <ScrollViewer Grid.Row="2" Visibility="Visible" Name="MenuScroll" Style="{StaticResource ScrollViewerBase.Style}">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="1*"/>
                    </Grid.RowDefinitions>
                    <TextBox Grid.Row="0" Name="TB_SelectedHouse" Style="{StaticResource TextBox_Info.Style}"/>
                    <TextBox Grid.Row="1" Name="TB_SelectedApartment" Style="{StaticResource TextBox_Info.Style}"/>
                    <TextBox Grid.Row="2" Name="TB_SelectedDebtor" Style="{StaticResource TextBox_Info.Style}"/>
                    <TextBox Grid.Row="3" Name="TB_SelectedArrear" Style="{StaticResource TextBox_Info.Style}"/>
                </Grid>
            </ScrollViewer>
            <Button Grid.Row="3" Content="Сбросить фильтр" Command="{Binding WipeCommand}"  Style="{StaticResource Button_Menu.Style}"/>
        </Grid>
        <Grid Grid.Column="1">
            <Grid Name="HousesForm" Visibility="Hidden">
                <Grid.RowDefinitions>
                    <RowDefinition Height="9*"/>
                    <RowDefinition Height="1*"/>
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="3*"/>
                    </Grid.ColumnDefinitions>

                    <DataGrid Grid.Column="0"  Name="HousesTable"
                          Style="{StaticResource EntitiesTable}"
                          ItemsSource="{Binding AllHouses}"
                          SelectedItem="{Binding Path=SelectedHouse, Mode=TwoWay}">
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="Город" Binding="{Binding Path=City}" ElementStyle="{StaticResource DataGridCellStyle}"/>
                            <DataGridTextColumn Header="Улица" Binding="{Binding Path=Street}" ElementStyle="{StaticResource DataGridCellStyle}"/>
                            <DataGridTextColumn Header="Дом" Binding="{Binding Path=NumberHouse}" ElementStyle="{StaticResource DataGridCellStyle}"/>                            
                        </DataGrid.Columns>
                    </DataGrid>

                    <ScrollViewer Grid.Column="1" Name="HouseProperties" DataContext="{Binding SelectedHouse}" Style="{StaticResource ScrollViewer_PropertiesEntity.Style}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="9*"/>
                            </Grid.RowDefinitions>
                            <Expander Grid.Row="0" Header="Адрес дома" Style="{StaticResource Expander_Expanded.Style}">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="1*"/>
                                        <RowDefinition Height="1*"/>
                                        <RowDefinition Height="1*"/>
                                        <RowDefinition Height="1*"/>
                                        <RowDefinition Height="1*"/>
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>

                                        <Border Height="{Binding ActualHeight, ElementName=TB_HouseIndex}" Style="{StaticResource Border_OneString.Style}">
                                            <TextBlock Grid.Column="0" Text="Индекс: " Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                        </Border>
                                        <TextBox Grid.Column="1" x:Name="TB_HouseIndex" Text="{Binding Path=Index}" Style="{StaticResource TextBox_PropertiesEntity.Style}" />
                                    </Grid>
                                    <Grid Grid.Row="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>

                                        <Border Height="{Binding ActualHeight, ElementName=TB_HouseCity}" Style="{StaticResource Border_OneString.Style}">
                                            <TextBlock Grid.Column="0" Text="Город: " Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                        </Border>
                                        <TextBox Grid.Column="1" x:Name="TB_HouseCity" Text="{Binding Path=City}" Style="{StaticResource TextBox_PropertiesEntity.Style}" />
                                    </Grid>
                                    <Grid Grid.Row="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>

                                        <Border Height="{Binding ActualHeight, ElementName=TB_HouseStreet}" Style="{StaticResource Border_OneString.Style}">
                                            <TextBlock Grid.Column="0" Text="Улица: " Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                        </Border>
                                        <TextBox Grid.Column="1" x:Name="TB_HouseStreet" Text="{Binding Path=Street}" Style="{StaticResource TextBox_PropertiesEntity.Style}" />
                                    </Grid>
                                    <Grid Grid.Row="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>

                                        <Border Height="{Binding ActualHeight, ElementName=TB_HouseNumber}" Style="{StaticResource Border_OneString.Style}">
                                            <TextBlock Grid.Column="0" Text="Дом: " Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                        </Border>
                                        <TextBox Grid.Column="1" x:Name="TB_HouseNumber" Text="{Binding Path=NumberHouse}" Style="{StaticResource TextBox_PropertiesEntity.Style}" />
                                    </Grid>
                                    <Grid Grid.Row="4">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>

                                        <Border Height="{Binding ActualHeight, ElementName=TB_HouseCorpus}" Style="{StaticResource Border_OneString.Style}">
                                            <TextBlock Grid.Column="0" Text="Корпус: " Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                        </Border>
                                        <TextBox Grid.Column="1" x:Name="TB_HouseCorpus" Text="{Binding Path=Corpus}" Style="{StaticResource TextBox_PropertiesEntity.Style}" />
                                    </Grid>
                                </Grid>
                            </Expander>                            
                        </Grid>
                    </ScrollViewer>
                </Grid>
                <Grid Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <Button Grid.Column="0" Name="Button_Add_House" Command="{Binding AddCommand}" Style="{StaticResource Button_Add.Style}"/>
                    <Button Grid.Column="1" Name="Button_Save_House" Command="{Binding SaveCommand}" Style="{StaticResource Button_Save.Style}"/>
                    <Button Grid.Column="2" Name="Button_Remove_House" Command="{Binding RemoveCommand}" Style="{StaticResource Button_Remove.Style}"/>
                    <Button Grid.Column="3" Name="Button_Pick_House" Command="{Binding PickCommand}" Style="{StaticResource Button_Pick.Style}"/>
                </Grid>
            </Grid>

            <Grid Name="ApartmentsForm" Visibility="Hidden">
                <Grid.RowDefinitions>
                    <RowDefinition Height="9*"/>
                    <RowDefinition Height="1*"/>
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="3*"/>
                    </Grid.ColumnDefinitions>

                    <Grid Grid.Column="0">
                        <DataGrid Name="ApartmentsTable"
                              Style="{StaticResource EntitiesTable}"
                              ItemsSource="{Binding AllApartments}"
                              SelectedItem="{Binding Path=SelectedApartment, Mode=TwoWay}">
                            <DataGrid.Columns>
                                <DataGridTextColumn Header="Тип" Binding="{Binding Path=Type}" ElementStyle="{StaticResource DataGridCellStyle}"/>
                                <DataGridTextColumn Header="№" Binding="{Binding Path=EstateNumber}" ElementStyle="{StaticResource DataGridCellStyle}"/>
                            </DataGrid.Columns>
                        </DataGrid>
                    </Grid>

                    <ScrollViewer Grid.Column="1" Name="ApartmentProperties" DataContext="{Binding SelectedApartment}" Style="{StaticResource ScrollViewer_PropertiesEntity.Style}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="8*"/>
                            </Grid.RowDefinitions>

                            <Expander Grid.Row="0" Header="Выписки ЕГРН на объект недвижимости" Style="{StaticResource Expander_Expanded.Style}">
                                <DataGrid IsEnabled="False" Name="ApartmentExtractsTable"
                              Style="{StaticResource EntitiesTable}"                             
                              ItemsSource="{Binding Extracts}">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Фамилия владельца" Binding="{Binding Path=Debtor.LastName}" ElementStyle="{StaticResource DataGridCellStyle}"/>
                                        <DataGridTextColumn Header="Имя владельца" Binding="{Binding Path=Debtor.FirstName}" ElementStyle="{StaticResource DataGridCellStyle}"/>
                                        <DataGridTextColumn Header="Отчество владельца" Binding="{Binding Path=Debtor.MiddleName}" ElementStyle="{StaticResource DataGridCellStyle}"/>
                                        <DataGridTextColumn Header="№ выписки" Binding="{Binding Path=NumberExtractEGRN}" ElementStyle="{StaticResource DataGridCellStyle}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Expander>

                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="2*"/>
                                </Grid.ColumnDefinitions>

                                <Border Height="{Binding ActualHeight, ElementName=CB_ApartmentType}" Style="{StaticResource Border_OneString.Style}">
                                    <TextBlock Grid.Column="0" Text="Тип имущества" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                </Border>
                                <ComboBox Grid.Column="1" Name="CB_ApartmentType" Text="{Binding Path=Type}" Style="{StaticResource ComboBox_PropertiesEntity.Style}"/>
                            </Grid>
                            <Grid Grid.Row="2">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="2*"/>
                                </Grid.ColumnDefinitions>

                                <Border Height="{Binding ActualHeight, ElementName=TB_ApartmentNumber}" Style="{StaticResource Border_OneString.Style}">
                                    <TextBlock Grid.Column="0" Text="Номер имущества" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                </Border>
                                <TextBox Grid.Column="1" Name="TB_ApartmentNumber" Text="{Binding Path=EstateNumber}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                            </Grid>
                        </Grid>
                    </ScrollViewer>
                </Grid>
                <Grid Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <Button Grid.Column="0" Name="Button_Add_Apartment" Command="{Binding AddCommand}" Style="{StaticResource Button_Add.Style}"/>
                    <Button Grid.Column="1" Name="Button_Save_Apartment" Command="{Binding SaveCommand}" Style="{StaticResource Button_Save.Style}" />
                    <Button Grid.Column="2" Name="Button_Remove_Apartment" Command="{Binding RemoveCommand}" Style="{StaticResource Button_Remove.Style}" />
                    <Button Grid.Column="3" Name="Button_Pick_Apartment" Command="{Binding PickCommand}" Style="{StaticResource Button_Pick.Style}"/>
                </Grid>
            </Grid>

            <Grid Name="DebtorsForm" Visibility="Hidden">
                <Grid.RowDefinitions>
                    <RowDefinition Height="9*"/>
                    <RowDefinition Height="1*"/>
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="3*"/>
                    </Grid.ColumnDefinitions>

                    <Grid Grid.Column="0">
                        <DataGrid Name="DebtorsTable"
                              Style="{StaticResource EntitiesTable}"                             
                              ItemsSource="{Binding AllDebtors}"
                              SelectedItem="{Binding Path=SelectedDebtor, Mode=TwoWay}">
                            <DataGrid.Columns>
                                <DataGridTextColumn Header="Фамилия" Binding="{Binding Path=LastName}" ElementStyle="{StaticResource DataGridCellStyle}"/>
                                <DataGridTextColumn Header="Имя" Binding="{Binding Path=FirstName}" ElementStyle="{StaticResource DataGridCellStyle}"/>
                                <DataGridTextColumn Header="Отчество" Binding="{Binding Path=MiddleName}" ElementStyle="{StaticResource DataGridCellStyle}"/>                                
                            </DataGrid.Columns>
                        </DataGrid>
                    </Grid>

                    <ScrollViewer Grid.Column="1" Name="DebtorProperties" DataContext="{Binding SelectedDebtor}" Style="{StaticResource ScrollViewer_PropertiesEntity.Style}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="15*"/>
                            </Grid.RowDefinitions>
                            <Expander Grid.Row="0" Header="Выписки ЕГРН у должника" Style="{StaticResource Expander_Expanded.Style}">
                                <DataGrid IsEnabled="False" Name="DebtorExtractsTable"
                                    Style="{StaticResource EntitiesTable}"                             
                                    ItemsSource="{Binding Extracts}">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="№ имущества" Binding="{Binding Path=Apartment.EstateNumber}" ElementStyle="{StaticResource DataGridCellStyle}"/>
                                        <DataGridTextColumn Header="№ выписки" Binding="{Binding Path=NumberExtractEGRN}" ElementStyle="{StaticResource DataGridCellStyle}"/>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Expander>

                            <Expander Grid.Row="1" Header="Основная информация" Style="{StaticResource Expander_Expanded.Style}">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="1*"/>
                                        <RowDefinition Height="1*"/>
                                        <RowDefinition Height="1*"/>
                                        <RowDefinition Height="1*"/>
                                        <RowDefinition Height="1*"/>
                                        <RowDefinition Height="1*"/>
                                        <RowDefinition Height="1*"/>
                                        <RowDefinition Height="1*"/>
                                    </Grid.RowDefinitions>

                                    <Grid Grid.Row="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>

                                        <Border Height="{Binding ActualHeight, ElementName=TB_DebtorLastName}" Style="{StaticResource Border_OneString.Style}">
                                            <TextBlock Grid.Column="0" Text="Фамилия :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                        </Border>
                                        <TextBox Grid.Column="1" Name="TB_DebtorLastName" Text="{Binding Path=LastName}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                                    </Grid>
                                    <Grid Grid.Row="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>

                                        <Border Height="{Binding ActualHeight, ElementName=TB_DebtorFirstName}" Style="{StaticResource Border_OneString.Style}">
                                            <TextBlock Grid.Column="0" Text="Имя :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                        </Border>
                                        <TextBox Grid.Column="1" Name="TB_DebtorFirstName" Text="{Binding Path=FirstName}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                                    </Grid>
                                    <Grid Grid.Row="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>

                                        <Border Height="{Binding ActualHeight, ElementName=TB_DebtorMiddleName}" Style="{StaticResource Border_OneString.Style}">
                                            <TextBlock Grid.Column="0" Text="Отчество :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                        </Border>
                                        <TextBox Grid.Column="1" Name="TB_DebtorMiddleName" Text="{Binding Path=MiddleName}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                                    </Grid>
                                    <Grid Grid.Row="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>

                                        <Border Height="{Binding ActualHeight, ElementName=CB_GenderType}" Style="{StaticResource Border_OneString.Style}">
                                            <TextBlock Grid.Column="0" Text="Пол :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                        </Border>
                                        <ComboBox Grid.Column="1" Name="CB_GenderType" Text="{Binding Path=Gender}" Style="{StaticResource ComboBox_PropertiesEntity.Style}"/>
                                    </Grid>
                                    <Grid Grid.Row="4">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>

                                        <Border Height="{Binding ActualHeight, ElementName=TB_DebtorBirthDate}" Style="{StaticResource Border_OneString.Style}">
                                            <TextBlock Grid.Column="0" Text="Дата рождения :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                        </Border>
                                        <TextBox Grid.Column="1" Name="TB_DebtorBirthDate" Text="{Binding Path=BirthDate}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                                    </Grid>

                                    <Grid Grid.Row="5">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>

                                        <Border Height="{Binding ActualHeight, ElementName=TB_DebtorBirthPlace}" Style="{StaticResource Border_OneString.Style}">
                                            <TextBlock Grid.Column="0" Text="Место рождения :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                        </Border>
                                        <TextBox Grid.Column="1" Name="TB_DebtorBirthPlace" Text="{Binding Path=BirthPlace}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                                    </Grid>
                                </Grid>
                            </Expander>
                            <Expander Grid.Row="2" Header="Паспортные данные" Style="{StaticResource Expander_Expanded.Style}">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="1*"/>
                                        <RowDefinition Height="1*"/>
                                        <RowDefinition Height="1*"/>
                                        <RowDefinition Height="1*"/>
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>

                                        <Border Height="{Binding ActualHeight, ElementName=TB_DebtorPassportSerial}" Style="{StaticResource Border_OneString.Style}">
                                            <TextBlock Grid.Column="0" Text="Серия :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                        </Border>
                                        <TextBox Grid.Column="1" Name="TB_DebtorPassportSerial" Text="{Binding Path=PassportSerial}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                                    </Grid>
                                    <Grid Grid.Row="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>

                                        <Border Height="{Binding ActualHeight, ElementName=TB_DebtorPassportNumber}" Style="{StaticResource Border_OneString.Style}">
                                            <TextBlock Grid.Column="0" Text="Номер :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                        </Border>
                                        <TextBox Grid.Column="1" Name="TB_DebtorPassportNumber" Text="{Binding Path=PassportNumber}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                                    </Grid>
                                    <Grid Grid.Row="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>

                                        <Border Height="{Binding ActualHeight, ElementName=TB_DebtorPassportDate}" Style="{StaticResource Border_OneString.Style}">
                                            <TextBlock Grid.Column="0" Text="Дата выдачи :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                        </Border>
                                        <TextBox Grid.Column="1" Name="TB_DebtorPassportDate" Text="{Binding Path=PassportDate}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                                    </Grid>
                                    <Grid Grid.Row="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>

                                        <Border Height="{Binding ActualHeight, ElementName=TB_DebtorPassportIssueBy}" Style="{StaticResource Border_OneString.Style}">
                                            <TextBlock Grid.Column="0" Text="Кем выдан :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                        </Border>
                                        <TextBox Grid.Column="1" Name="TB_DebtorPassportIssueBy" Text="{Binding Path=PassportIssueBy}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                                    </Grid>
                                </Grid>
                            </Expander>
                            <Expander Grid.Row="3" Header="Контактные данные" Style="{StaticResource Expander_Expanded.Style}">
                                <Grid>
