                                                <TextBlock Grid.Column="0" Text="Госпошлина :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                            </Border>
                                            <TextBox Grid.Column="1" Name="TB_ArrearDutyGovernment" Text="{Binding Path=DutyGovernment}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                                        </Grid>
                                        <Grid Grid.Row="7">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="1*"/>
                                                <ColumnDefinition Width="2*"/>
                                            </Grid.ColumnDefinitions>

                                            <Border Height="{Binding ActualHeight, ElementName=TB_ArrearDutyDelegateServices}" Style="{StaticResource Border_OneString.Style}">
                                                <TextBlock Grid.Column="0" Text="Услуги представителя :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                            </Border>
                                            <TextBox Grid.Column="1" Name="TB_ArrearDutyDelegateServices" Text="{Binding Path=DutyDelegateServices}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                                        </Grid>
                                    </Grid>
                                </Expander>

                                <Grid Grid.Row="9">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="1*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <Border Height="{Binding ActualHeight, ElementName=CB_LifeCycleArrear}" Style="{StaticResource Border_OneString.Style}">
                                        <TextBlock Grid.Column="0" Text="Этап задолженности :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                    </Border>
                                    <ComboBox Grid.Column="1" Name="CB_LifeCycleArrear" Text="{Binding Path=LifeCycle}" Style="{StaticResource ComboBox_PropertiesEntity.Style}"/>
                                </Grid>
                            </Grid>
                        </Grid>
                    </ScrollViewer>
                </Grid>
                <Grid Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <Button Grid.Column="0" Name="Button_Add_Arrear" Command="{Binding AddCommand}" Style="{StaticResource Button_Add.Style}"/>
                    <Button Grid.Column="1" Name="Button_Save_Arrear" Command="{Binding SaveCommand}" Style="{StaticResource Button_Save.Style}"/>
                    <Button Grid.Column="2" Name="Button_Remove_Arrear" Command="{Binding RemoveCommand}" Style="{StaticResource Button_Remove.Style}"/>
                    <Button Grid.Column="3" Name="Button_Pick_Arrear" Command="{Binding PickCommand}" Style="{StaticResource Button_Pick.Style}"/>
                </Grid>
                <Grid Grid.Row="2">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>

                    <Button Grid.Column="0" Name="Button_CreateSP" Visibility="Hidden" Content="Создать СП" Command="{Binding CreateSPCommand}" Style="{StaticResource Button_Shablon.Style}"/>
                    <Button Grid.Column="1" Name="Button_CreateIP" Visibility="Hidden" Content="Создать ИП" Command="{Binding CreateIPCommand}" Style="{StaticResource Button_Shablon.Style}"/>

                    <Grid Grid.Column="3" Name="ProgressGrid" Visibility="Hidden">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1*"/>
                            <RowDefinition Height="1*"/>
                        </Grid.RowDefinitions>
                        <ProgressBar Name="ProgressInfo" Minimum="0" Maximum="100" Margin="5"/>
                    </Grid>
                </Grid>
            </Grid>

            <Grid Name="ExtractsForm" Visibility="Hidden">
                <Grid.RowDefinitions>
                    <RowDefinition Height="9*"/>
                    <RowDefinition Height="1*"/>
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="3*"/>
                    </Grid.ColumnDefinitions>

                    <Grid Grid.Column="0">
                        <DataGrid Name="ExtractsTable"
                              Style="{StaticResource EntitiesTable}"
                              ItemsSource="{Binding AllExtracts}"
                              SelectedItem="{Binding Path=SelectedExtract, Mode=TwoWay}">
                            <DataGrid.Columns>
                                <DataGridTextColumn Header="№ имущества" Binding="{Binding Path=Apartment.EstateNumber}" ElementStyle="{StaticResource DataGridCellStyle}"/>
                                <DataGridTextColumn Header="Фамилия владельца" Binding="{Binding Path=Debtor.LastName}" ElementStyle="{StaticResource DataGridCellStyle}"/>
                                <DataGridTextColumn Header="Имя владельца" Binding="{Binding Path=Debtor.FirstName}" ElementStyle="{StaticResource DataGridCellStyle}"/>
                            </DataGrid.Columns>
                        </DataGrid>
                    </Grid>

                    <ScrollViewer Grid.Column="1" Name="ExtractProperties" DataContext="{Binding SelectedExtract}" Style="{StaticResource ScrollViewerBase.Style}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="10*"/>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="2*"/>
                                </Grid.ColumnDefinitions>

                                <Border Height="{Binding ActualHeight, ElementName=TB_NumberExtractEGRN}" Style="{StaticResource Border_OneString.Style}">
                                    <TextBlock Grid.Column="0" Text="Номер выписки :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                </Border>
                                <TextBox Grid.Column="1" Name="TB_NumberExtractEGRN" Text="{Binding Path=NumberExtractEGRN}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                            </Grid>
                            <Expander Grid.Row="1" Header="Данные о выписке" Style="{StaticResource Expander_Expanded.Style}">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="1*"/>
                                        <RowDefinition Height="1*"/>
                                        <RowDefinition Height="1*"/>
                                        <RowDefinition Height="1*"/>
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>

                                        <Border Height="{Binding ActualHeight, ElementName=TB_ExtractNumber}" Style="{StaticResource Border_OneString.Style}">
                                            <TextBlock Grid.Column="0" Text="Номер имущества :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                        </Border>
                                        <TextBox Grid.Column="1" Name="TB_ExtractNumber" Text="{Binding Path=Apartment.EstateNumber}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                                    </Grid>
                                    <Grid Grid.Row="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>

                                        <Border Height="{Binding ActualHeight, ElementName=TB_LastNameOwner}" Style="{StaticResource Border_OneString.Style}">
                                            <TextBlock Grid.Column="0" Text="Фамилия владельца :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                        </Border>
                                        <TextBox Grid.Column="1" Name="TB_LastNameOwner" Text="{Binding Path=Debtor.LastName}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                                    </Grid>
                                    <Grid Grid.Row="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>

                                        <Border Height="{Binding ActualHeight, ElementName=TB_FirstNameOwner}" Style="{StaticResource Border_OneString.Style}">
                                            <TextBlock Grid.Column="0" Text="Имя владельца :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                        </Border>
                                        <TextBox Grid.Column="1" Name="TB_FirstNameOwner" Text="{Binding Path=Debtor.FirstName}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                                    </Grid>
                                    <Grid Grid.Row="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>

                                        <Border Height="{Binding ActualHeight, ElementName=TB_MiddleNameOwner}" Style="{StaticResource Border_OneString.Style}">
                                            <TextBlock Grid.Column="0" Text="Отчество владельца :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                        </Border>
                                        <TextBox Grid.Column="1" Name="TB_MiddleNameOwner" Text="{Binding Path=Debtor.MiddleName}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                                    </Grid>
                                </Grid>
                            </Expander>
                        </Grid>
                    </ScrollViewer>
                </Grid>
                <Grid Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <Button Grid.Column="0" Name="Button_Add_Extract" Command="{Binding AddCommand}" Style="{StaticResource Button_Add.Style}"/>
                    <Button Grid.Column="1" Name="Button_Save_Extract" Command="{Binding SaveCommand}" Style="{StaticResource Button_Save.Style}" />
                    <Button Grid.Column="2" Name="Button_Remove_Extract" Command="{Binding RemoveCommand}" Style="{StaticResource Button_Remove.Style}"/>
                </Grid>
            </Grid>

            <Grid Name="SettingsForm" Visibility="Hidden">
                <Grid.RowDefinitions>
                    <RowDefinition Height="9*"/>
                    <RowDefinition Height="1*"/>
                </Grid.RowDefinitions>
                <ScrollViewer Style="{StaticResource ScrollViewerBase.Style}">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1*"/>
                            <RowDefinition Height="11*"/>
                        </Grid.RowDefinitions>
                        <Expander Grid.Row="0" Header="Настройки путей" Style="{StaticResource Expander_Expanded.Style}">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="1*"/>
                                    <RowDefinition Height="1*"/>
                                    <RowDefinition Height="1*"/>
                                    <RowDefinition Height="1*"/>
                                    <RowDefinition Height="1*"/>
                                    <RowDefinition Height="1*"/>
                                    <RowDefinition Height="1*"/>
                                </Grid.RowDefinitions>
                                <Grid Grid.Row="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="8*"/>
                                        <ColumnDefinition Width="1*"/>
                                    </Grid.ColumnDefinitions>

                                    <Border Height="{Binding ActualHeight, ElementName=NameFileShablonSP}" Style="{StaticResource Border_Settings.Style}">
                                        <TextBlock Grid.Column="0" Text="Шаблон (судебный приказ) :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                    </Border>
                                    <TextBox Grid.Column="1" Name="NameFileShablonSP" Text="{Binding PathFileShablonSP}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                                    <Button Grid.Column="2" Name="ButtonPickShablonSP" Style="{StaticResource Button_Settings_Popup.Style}" Command="{Binding PickShablonSPCommand}"/>
                                </Grid>
                                <Grid Grid.Row="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="8*"/>
                                        <ColumnDefinition Width="1*"/>
                                    </Grid.ColumnDefinitions>

                                    <Border Height="{Binding ActualHeight, ElementName=NameFolderResultSP}" Style="{StaticResource Border_Settings.Style}">
                                        <TextBlock Grid.Column="0" Text="Папка для хранения заявлений (судебный приказ) :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                    </Border>
                                    <TextBox Grid.Column="1" Name="NameFolderResultSP" Text="{Binding PathFolderResultSP}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                                    <Button Grid.Column="2" Name="ButtonPickFolderResultSP" Style="{StaticResource Button_Settings_Popup.Style}" Command="{Binding PickFolderResultSPCommand}"/>
                                </Grid>
                                <Grid Grid.Row="3">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="8*"/>
                                        <ColumnDefinition Width="1*"/>
                                    </Grid.ColumnDefinitions>

                                    <Border Height="{Binding ActualHeight, ElementName=NameFileShablonIP}" Style="{StaticResource Border_Settings.Style}">
                                        <TextBlock Grid.Column="0" Text="Шаблон (исполнительное производство) :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                    </Border>
                                    <TextBox Grid.Column="1" Name="NameFileShablonIP" Text="{Binding PathFileShablonIP}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                                    <Button Grid.Column="2" Name="ButtonPickShablonIP" Style="{StaticResource Button_Settings_Popup.Style}" Command="{Binding PickShablonIPCommand}"/>
                                </Grid>
                                <Grid Grid.Row="4">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="8*"/>
                                        <ColumnDefinition Width="1*"/>
                                    </Grid.ColumnDefinitions>

                                    <Border Height="{Binding ActualHeight, ElementName=NameFolderResultIP}" Style="{StaticResource Border_Settings.Style}">
                                        <TextBlock Grid.Column="0" Text="Папка для хранения заявлений (исполнительное производство) :" Style="{StaticResource TextBlock_PropertiesEntity.Style}"/>
                                    </Border>
                                    <TextBox Grid.Column="1" Name="NameFolderResultIP" Text="{Binding PathFolderResultIP}" Style="{StaticResource TextBox_PropertiesEntity.Style}"/>
                                    <Button Grid.Column="2" Name="ButtonPickFolderResultIP" Style="{StaticResource Button_Settings_Popup.Style}" Command="{Binding PickFolderResultIPCommand}"/>
                                </Grid>
                            </Grid>
                        </Expander>
                    </Grid>
                </ScrollViewer>
                <Grid Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <Button Grid.Column="1" Name="ButtonSave_Settings" Style="{StaticResource Button_Save.Style}" Command="{Binding SavePathsCommand}"/>
                    <Button Grid.Column="2" Name="ButtonReturn_Settings" Content="Откатить изменения" 
                            Style="{StaticResource Button_Remove.Style}" Command="{Binding ReturnPathsCommand}"/>
                </Grid>
            </Grid>
        </Grid>
    </Grid>
</Window>
